{
  "compilerOptions": {
    "target": "es5",
    "lib": [],
    "allowJs": false,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "declaration": false,
    "sourceMap": true,
    "baseUrl": ".",
    "rootDir": "."
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./**/*.ts",
  ]
}